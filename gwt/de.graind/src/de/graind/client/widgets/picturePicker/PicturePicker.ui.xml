<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.main {
		}
		.main div {
		}
		.medium {
			color: #888;
			font-size: 2em;
			opacity: 0.5;
		}
		.normal {
			color: #FFF;
			font-size: 1.3em;
			opacity: 0.5;
		}
		.west {
			background-color: #151515;
		}
		.center {
			border-left: 1px solid #333;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:DockLayoutPanel unit="PX" height="100%" width="100%" styleName="{style.main}">
			<g:west size="200.0">
				<g:LayoutPanel  styleName="{style.west}">
					<g:layer top="0px" bottom="0px" left="0px" right="0px">
						<g:VerticalPanel>
						</g:VerticalPanel>
					</g:layer>
					<g:layer top="-1ex" height="4em" right="0px" width="100%">
						<g:Label styleName="{style.medium}" wordWrap="false" horizontalAlignment="ALIGN_RIGHT" text="Selector"/>
					</g:layer>
				</g:LayoutPanel>
			</g:west>
			<g:center>
				<g:SimplePanel ui:field="picSpace" styleName="{style.center}"/>
			</g:center>
		</g:DockLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 